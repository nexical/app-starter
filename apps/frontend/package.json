{
  "name": "@app/frontend",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "generate:version": "npx tsx ../../scripts/generate-version.ts",
    "setup": "npm run generate:version && npm run setup:theme --workspace=app-core",
    "build": "npm run setup && astro build",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "npx vitest run -c vitest.unit.config.ts --passWithNoTests",
    "test:e2e": "npx playwright test",
    "dev": "npm run setup && astro dev",
    "preview": "astro preview"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "app-core": "*",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.5",
    "@astrojs/react": "^4.4.2",
    "@nexical/sdk": "*",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.17.1",
    "defu": "^6.1.4"
  }
}